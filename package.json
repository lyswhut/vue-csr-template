{
  "name": "vue-spa-template",
  "version": "1.0.0",
  "description": "A vue-CSR based scaffolding.",
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production webpack --config build-config/webpack.config.prod.js --progress --hide-modules",
    "clean": "rimraf public",
    "build": "npm run clean && npm run build:prod",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build-config/webpack.config.dev.js",
    "lint": "eslint -f ./node_modules/eslint-formatter-friendly --ext .js --ext .jsx --ext .vue src/",
    "lint-fix": "eslint -f ./node_modules/eslint-formatter-friendly --fix --ext .js --ext .jsx --ext .vue src/"
  },
  "browserslist": [
    ">= 1%",
    "last 1 major version",
    "not dead",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyswhut/vue-spa-template.git"
  },
  "keywords": [
    "vue"
  ],
  "author": "lyswhut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lyswhut/vue-spa-template/issues"
  },
  "homepage": "https://github.com/lyswhut/vue-spa-template#readme",
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-umd": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.1",
    "core-js": "^3.7.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.5.0",
    "less": "^3.12.2",
    "less-loader": "^7.1.0",
    "mini-css-extract-plugin": "^0.11.3",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^3.0.0",
    "pug": "^3.0.1",
    "pug-plain-loader": "^1.0.0",
    "rimraf": "^3.0.2",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "^4.2.3",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.5",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.0"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "vue": "^2.6.12",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.4.9",
    "vuex": "^3.5.1",
    "vuex-router-sync": "^5.0.0"
  }
}
