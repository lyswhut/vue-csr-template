{
  "name": "vue-spa-template",
  "version": "1.0.0",
  "description": "A vue-CSR based scaffolding.",
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production webpack --config build-config/webpack.config.prod.js --progress",
    "clean": "rimraf public",
    "build": "npm run clean && npm run build:prod",
    "dev": "cross-env NODE_ENV=development webpack serve --config build-config/webpack.config.dev.js",
    "lint": "eslint -f ./node_modules/eslint-formatter-friendly --ext .js --ext .jsx --ext .vue src/",
    "lint-fix": "eslint -f ./node_modules/eslint-formatter-friendly --fix --ext .js --ext .jsx --ext .vue src/"
  },
  "browserslist": [
    ">= 1%",
    "last 1 major version",
    "not dead",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyswhut/vue-spa-template.git"
  },
  "keywords": [
    "vue"
  ],
  "author": "lyswhut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lyswhut/vue-spa-template/issues"
  },
  "homepage": "https://github.com/lyswhut/vue-spa-template#readme",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-umd": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.15.0",
    "autoprefixer": "^10.3.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "core-js": "^3.16.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^5.3.2",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "less-plugin-clean-css": "^1.5.1",
    "mini-css-extract-plugin": "^2.2.0",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "rimraf": "^3.0.2",
    "stylus": "^0.54.8",
    "stylus-loader": "^6.1.0",
    "terser-webpack-plugin": "^5.1.4",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "vue": "^2.6.14",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.5.2",
    "vuex": "^3.6.2",
    "vuex-router-sync": "^5.0.0"
  }
}
